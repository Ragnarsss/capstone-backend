{
    "name": "hawaii/jwt-bridge-service",
    "description": "Servicio minimalista para generaciÃ³n segura de JWT",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "ext-redis": "Required for rate limiting functionality in production"
    },
    "autoload": {
        "psr-4": {
            "JwtBridge\\": "src/"
        },
        "classmap": [
            "src/middleware/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JwtBridge\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    }
}